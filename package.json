  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "vite",
    "dev:server": "nodemon --exec ts-node --esm server/index.ts",
    "build": "tsc && vite build",
    "build:server": "tsc --project server/tsconfig.json",
    "lint": "eslint .",
